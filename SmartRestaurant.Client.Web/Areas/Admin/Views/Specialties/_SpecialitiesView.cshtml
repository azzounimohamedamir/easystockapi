@model IEnumerable<SpecialityItemModel>
@{
    if (Model.Any())
    {
        <table class="table">
            <thead class="thead-inverse">
                <tr>                    
                    <th>
                        @BaseResource.Name
                    </th>                    
                    <th>
                        @BaseResource.IsDisabled
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <partial name="_SpecialityListItemView" model="item" />
                }
            </tbody>
        </table>
    }
    else
    {
        <h4>@UtilsResource.EmptyData</h4>
    }
}