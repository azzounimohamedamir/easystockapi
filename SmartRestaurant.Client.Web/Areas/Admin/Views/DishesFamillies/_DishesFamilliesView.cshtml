@model IEnumerable<DishFamilyItemModel>
@{
    if (Model.Any())
    {
        <table class="table">
            <thead class="thead-inverse">
                <tr>
                    <th class="marge"></th>
                    <th>
                        @BaseResource.PictureUrl
                    </th>
                    <th>
                        @BaseResource.Name
                    </th>
                    <th>
                        @DishFamilyResource.ParentId
                    </th>
                    <th>
                        @DishFamilyResource.RestaurantId
                    </th>
                    <th>
                        @BaseResource.IsDisabled
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <partial name="_DishFamilyListItemView" model="item" />
                }
            </tbody>
        </table>
    }
    else
    {
        <h4>@UtilsResource.EmptyData</h4>
    }
}