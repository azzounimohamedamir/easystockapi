@model IEnumerable<TemplateItemModel>

    <table class="table">
        <thead class="thead-inverse">
            <tr>
                <th>
                    @BaseResource.Name
                </th>
                <th>
                    @TemplateResource.Type
                </th>
                <th>
                    @BaseResource.Alias
                </th>
                <th>
                    @BaseResource.Description
                </th>
                
                <th>
                    @BaseResource.IsDisabled
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <partial name="_TemplateListItemView" model="item" />
}
        </tbody>
    </table>
