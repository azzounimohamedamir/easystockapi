@model IEnumerable<LanguageItemModel>

    <table class="table">
        <thead class="thead-inverse">
            <tr>
                <th>
                    @BaseResource.Name
                </th>
                <th>
                    @BaseResource.Alias
                </th>
                <th>
                    @BaseResource.IsoCode
                </th>
                <th>
                    @BaseResource.IsDisabled
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <partial name="_LanguageListItemView" model="item" />
}
        </tbody>
    </table>
