@model IEnumerable<NotificationItemModel>

<table class="table">
    <thead class="thead-inverse">
        <tr>
            <th>
                @BaseResource.Name
            </th>
            <th>
                @NotificationResource.TableName
            </th>
            <th>
                @NotificationResource.Action
            </th>
            <th>
                @NotificationResource.Users
            </th>
            <th>
                @BaseResource.IsDisabled
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <partial name="_NotificationListItemView" model="item" />
        }
    </tbody>
</table>
