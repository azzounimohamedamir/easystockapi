@using SmartRestaurant.Application.Restaurants.RestaurantTypes.Queries
@model IEnumerable<SmartRestaurant.Application.Restaurants.RestaurantTypes.Queries.RestaurantTypeItemModel>

<table class="table">
    <thead class="thead-inverse">
        <tr>
            <th>
                @BaseResource.Name
            </th>
            <th>
                @BaseResource.IsDisabled
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <partial name="_RestaurantTypeListItemView" model="item" />
            }
        }
    </tbody>
</table>
